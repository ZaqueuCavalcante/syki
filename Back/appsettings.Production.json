{
  "Auth": {
    "Issuer": "prod",
    "Audience": "prod",
    "ExpirationTimeInMinutes": 6000,
    "CookieSecure": false,
    "CookieDomain": ".syki.com.br"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Seq" ],
    "MinimumLevel": { "Default": "Information" },
    "WriteTo": [ { "Name": "Seq" } ],
    "Enrich": [ "FromLogContext", "WithMachineName" ]
  },
  "Sentry": {
    "TracesSampleRate": 0.50
  },
  "RateLimiter": {
    "SuperVerySmall": 2,
    "VerySmall": 10,
    "Small": 20,
    "Medium": 120
  }
}
